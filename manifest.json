{
  "manifest_version": 2,

  "name": "Select CSV",
  "description": "This extension parses highlighted text into downloadable CSV",
  "homepage_url": "https://github.com/jerobi/selectcsv",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Select CSV"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}
